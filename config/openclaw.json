{
  "models": {
    "bedrockDiscovery": {
      "enabled": true,
      "region": "eu-west-1",
      "providerFilter": ["anthropic"],
      "refreshInterval": 3600,
      "defaultContextWindow": 200000,
      "defaultMaxTokens": 8192
    },
    "providers": {
      "amazon-bedrock": {
        "baseUrl": "https://bedrock-runtime.eu-west-1.amazonaws.com",
        "api": "bedrock-converse-stream",
        "auth": "aws-sdk",
        "models": [
          {
            "id": "eu.anthropic.claude-sonnet-4-5-20250929-v1:0",
            "name": "Claude Sonnet 4.5 (Bedrock EU)",
            "reasoning": true,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "eu.anthropic.claude-3-7-sonnet-20250219-v1:0",
            "name": "Claude 3.7 Sonnet (Bedrock EU)",
            "reasoning": true,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "model": {
        "primary": "amazon-bedrock/eu.anthropic.claude-sonnet-4-5-20250929-v1:0",
        "fallbacks": ["amazon-bedrock/eu.anthropic.claude-3-7-sonnet-20250219-v1:0"]
      },
      "models": {
        "amazon-bedrock/eu.anthropic.claude-sonnet-4-5-20250929-v1:0": {
          "alias": "sonnet"
        },
        "amazon-bedrock/eu.anthropic.claude-3-7-sonnet-20250219-v1:0": {
          "alias": "sonnet37"
        }
      }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "port": 18789,
    "auth": {
      "token": ""
    }
  },
  "logging": {
    "level": "info",
    "consoleLevel": "info",
    "consoleStyle": "pretty"
  }
}
